{
  "name": "去除google的跟踪重定向",
  "version": "1.0",
  "description": "去除google的跟踪重定向.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background_page": "background.html",
  "permissions": [
    "tabs",
	"http://www.google.com.hk/*",
	"http://www.google.com/*",
	"https://www.google.com.hk/*",
	"https://www.google.com/*"
  ]
}

